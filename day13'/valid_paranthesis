class Solution {
public:
    bool isValid(string s) {
        stack<char> st;
        for(int i=0;i<s.size();i++)
        {
            if(s[i]=='[' or s[i]=='{' or s[i]=='(') st.push(s[i]);
            else if(s[i]==']') 
            {
                if(st.size()==0) return false;
                if(st.top()!='[') return false;
                st.pop();
            }
             else if(s[i]=='}') 
            {
                if(st.size()==0) return false;
                if(st.top()!='{') return false;
                st.pop();
            }
             else if(s[i]==')') 
            {
                if(st.size()==0) return false;
                if(st.top()!='(') return false;
                st.pop();
            }
        }
        if(st.size()!=0) return false;
        return true;
    }
};