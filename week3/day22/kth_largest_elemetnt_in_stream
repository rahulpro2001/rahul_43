     priority_queue<int, vector<int>, greater<int> > pq;
    int K ;
    KthLargest(int k, vector<int>& nums) {
        for(auto it:nums)
        {
            pq.push(it);
            if(pq.size()>k) pq.pop();
        }
        K = k;
    }
    
    int add(int val) {
        pq.push(val);
        if(pq.size()>K) pq.pop();
        return pq.top();
    }